@model CongThongTinDHL.Models.Logo

@{
    ViewData["Title"] = "Thêm Logo";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<h1>Thêm Mới</h1>

<h4>Logo</h4>
<hr />
<div class="row">
    <div class="col-md-6">
        <form method="post" enctype="multipart/form-data" asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label class="control-label">Chọn Logo</label>
                <input asp-for="ChoosePhoto" class="form-control" />
            </div>
            <div class="form-group mt-2 form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="Status" /> Hiển thị
                </label>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
                <a class="btn btn-light m-1" asp-action="Index">Quay Lại</a>
            </div>
        </form>
    </div>
</div>


